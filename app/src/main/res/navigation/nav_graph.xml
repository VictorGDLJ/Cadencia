<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/login"
        android:name="com.example.cadencia_tfg.ui.detalle.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_pagina_Inicio"
            app:destination="@id/pagina_Inicio" />
        <action
            android:id="@+id/action_login_to_olvidar"
            app:destination="@id/olvidar" />
    </fragment>
    <fragment
        android:id="@+id/pagina_Inicio"
        android:name="com.example.cadencia_tfg.ui.Pagina_Inicio"
        android:label="fragment_pagina__inicio"
        tools:layout="@layout/fragment_pagina__inicio" >
        <action
            android:id="@+id/action_pagina_Inicio_to_fragment_nuevo_habito"
            app:destination="@id/fragment_nuevo_habito" />
    </fragment>
    <fragment
        android:id="@+id/olvidar"
        android:name="com.example.cadencia_tfg.ui.detalle.Olvidar"
        android:label="fragment_olvidar"
        tools:layout="@layout/fragment_olvidar" />
    <fragment
        android:id="@+id/fragment_nuevo_habito"
        android:name="com.example.cadencia_tfg.ui.detalle.NuevoHabitoFragment"

        android:label="fragment_nuevo_habito"
        tools:layout="@layout/fragment_nuevo_habito" />

</navigation>